\NeedsTeXFormat{LaTeX2e}[1994/06/01]
\ProvidesPackage{beamercolorthemeHSR}[2013/09/28 Initial Version]

\RequirePackage{HSRColors}

\mode<presentation>
\setbeamercolor*{palette primary}{fg=HSRWhite, bg=HSRBlue}
\setbeamercolor*{title}{fg=HSRWhite, bg=HSRBlue}
\setbeamercolor*{frametitle}{parent={palette primary}}
\setbeamercolor*{itemize item}{fg=HSRBlue}
\setbeamercolor*{itemize subitem}{fg=HSRPetrol80}

\setbeamercolor*{enumerate item}{fg=HSRWhite, bg=HSRBlue}

\setbeamercolor*{section in toc}{fg=HSRSchwarz}
\setbeamercolor*{section in toc shaded}{fg=HSRSchwarz80}

\setbeamercolor*{section in head/foot}{fg=HSRBlue}

\setbeamercolor*{item projected}{fg=HSRWhite, bg=HSRBlue}

\mode<all>
